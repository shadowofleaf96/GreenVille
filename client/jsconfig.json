{
  "compilerOptions": {
    "target": "es6",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@/app/*": ["./app/*"],
      "@/components/*": ["./components/*"],
      "@/hooks/*": ["./hooks/*"],
      "@/lib/*": ["./lib/*"],
      "@/store/*": ["./store/*"],
      "@/services/*": ["./services/*"],
      "@/utils/*": ["./utils/*"],
      "@/frontoffice/*": ["./app/(frontoffice)/*"],
      "@/admin/*": ["./app/admin/*"]
    }
  },
  "jsx": "preserve",
  "include": ["**/*.js", "**/*.jsx"],
  "exclude": ["node_modules"]
}
